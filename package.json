{
  "name": "filer",
  "version": "1.0.0",
  "description": "Renames and moves files and directories in bulk.",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . Filer --version=0.36.8",
    "build-mac32": "electron-packager . Filer --version=0.36.8 --platform=darwin --arch=ia32 --icon=images/icon.icns",
    "build-mac64": "electron-packager . Filer --version=0.36.8 --platform=darwin --arch=x64 --icon=images/icon.icns",
    "build-win32": "electron-packager . Filer --version=0.36.8 --platform=win32 --arch=ia32 --icon=images/icon.ico",
    "build-win64": "electron-packager . Filer --version=0.36.8 --platform=win32 --arch=x64 --icon=images/icon.ico"
  },
  "author": "Christopher West",
  "license": "ISC",
  "devDependencies": {
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.36.8",
    "musicmetadata": "^2.0.2"
  }
}
